{
  "name": "effector-localstorage",
  "version": "0.0.0",
  "description": "Module for Effector for sync state with localStorage",
  "main": "index.js",
  "scripts": {
    "flow": "flow",
    "size": "size-limit",
    "lint": "eslint '**/*.{js,jsx}' --quiet",
    "prepublish": "npm run flow && npm run lint && npm run size"
  },
  "eslintConfig": {
    "extends": "@logux/eslint-config/browser",
    "rules": {
      "node/no-unpublished-require": "off",
      "es5/no-es6-static-methods": "off",
      "node/no-missing-require": "off",
      "func-style": "off"
    }
  },
  "size-limit": [
    {
      "limit": "124 B",
      "path": "index.js"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ilyalesik/effector-localstorage.git"
  },
  "keywords": [
    "effector",
    "localstorage"
  ],
  "author": "Ilya Lesik <ilialesik@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ilyalesik/effector-localstorage/issues"
  },
  "homepage": "https://github.com/ilyalesik/effector-localstorage#readme",
  "devDependencies": {
    "@logux/eslint-config": "^28.2.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-import-helpers": "^0.1.4",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.100.0",
    "size-limit": "^1.3.5"
  }
}
